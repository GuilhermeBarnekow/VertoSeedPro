    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.38",
    "expo-av": "~15.0.2",
    "expo-blur": "^14.0.3",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.14",
    "expo-font": "^13.0.3",
    "expo-haptics": "^14.0.1",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "^2.0.1",
    "expo-symbols": "^0.2.2",
