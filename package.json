{
  "name": "calculadora-verto",
  "main": "expo-router/entry",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 EXPO_USE_TUNNEL=1 expo start",
    "build:web": "expo export --platform web",
    "build:android": "eas build -p android --profile preview --no-wait",
    "build:production": "eas build -p android --profile production --non-interactive --no-wait",
    "submit:android": "eas submit -p android --latest",
    "lint": "expo lint",
    "clear-cache": "expo start --clear",
    "login": "npx eas-cli login",
    "register": "npx eas-cli register",
    "update": "npx eas-cli update"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-picker/picker": "^2.4.8",
